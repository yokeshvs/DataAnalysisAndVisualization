<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/database.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/database.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOG FAIL</from-outcome>
			<to-view-id>/logFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>menu</display-name>
		<from-view-id>/menu.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>showTable</display-name>
		<from-view-id>/home.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>RESET</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOG FAIL</from-outcome>
			<to-view-id>/logFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>uploadFile</display-name>
		<from-view-id>/queryUpload.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/queryUpload.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/queryUpload.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>RESET</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOG FAIL</from-outcome>
			<to-view-id>/logFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>showTable</display-name>
		<from-view-id>/dataAnalysis.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/dataAnalysis.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/dataAnalysis.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>RESET</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOG FAIL</from-outcome>
			<to-view-id>/logFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>showPlot</display-name>
		<from-view-id>/dataVisualization.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/dataVisualization.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/dataVisualization.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>RESET</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOG FAIL</from-outcome>
			<to-view-id>/logFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>showRegression</display-name>
		<from-view-id>/regressionAnalysis.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/regressionAnalysis.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/regressionAnalysis.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>RESET</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOG FAIL</from-outcome>
			<to-view-id>/logFail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>logFail</display-name>
		<from-view-id>/logFail.jsp</from-view-id>
		<navigation-case>
			<from-outcome>RESET</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
